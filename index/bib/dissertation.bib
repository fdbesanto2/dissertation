@incollection{holmes2004,
  langid = {english},
  title = {{{USING SIZE}}\textendash{{FREQUENCY DISTRIBUTIONS TO ANALYZE FIRE REGIMES IN FLORIDA}}},
  abstract = {Wildfire regimes in natural forest ecosystems have been characterized with power\textendash{}law distributions. In this paper, we evaluated whether wildfire regimes in a human-dominated landscape were also consistent with power\textendash{}law distributions. Our case study focused on wildfires in Florida, a state with rapid population growth and consequent rapid alteration of forest ecosystems and natural fire regimes. We found that all fire size\textendash{}frequency distributions evaluated in this study were consistent with power\textendash{}law distributions, but the power\textendash{}law distributions were piece-wise linear. A kink in the power\textendash{}law distributions occurred at about 640 ha for flatwoods fires and at about 290 ha for swamp fires. Above these levels, fires ``exploded'' into a catastrophic regime. If the kink represents the level at which fires become immune to fire suppression effort, we would expect that the location of the kink would occur at smaller fire sizes during extreme fire years due to the increased flammability of fuels and the relative scarcity of fire suppression resources. We found this result for three of four extreme fire years in flatwoods ecosystems and for all four extreme fire years in swamps. These results suggest that catastrophic fires may not be possible to prevent and that suppression efforts during extreme fire years may be best applied to strategic areas that decrease the connectivity of fuels.},
  booktitle = {Proceedings of the 22nd {{Tall Timbers Fire Ecology Conference}}: {{Fire}} in {{Temperate}}, {{Boreal}}, and {{Montane Ecosystems}}.},
  date = {2004},
  pages = {7},
  author = {Holmes, Thomas P and Prestemon, Jeffrey P and Pye, John M and Butry, David T and Mercer, D Evan and Abt, Karen L},
  file = {/Users/michaelkoontz/Zotero/storage/NA6VGCN4/Holmes et al. - USING SIZE–FREQUENCY DISTRIBUTIONS TO ANALYZE FIRE.pdf}
}

@article{kolb2016,
  langid = {english},
  title = {Observed and Anticipated Impacts of Drought on Forest Insects and Diseases in the {{United States}}},
  volume = {380},
  issn = {03781127},
