  urldate = {2019-03-14},
  date = {2019-02-16},
  pages = {384115},
  author = {Joseph, Maxwell B. and Rossi, Matthew W. and Mietkiewicz, Nathan P. and Mahood, Adam L. and Cattau, Megan E. and Denis, Lise Ann St and Nagy, R. Chelsea and Iglesias, Virginia and Abatzoglou, John T. and Balch, Jennifer K.},
  file = {/Users/michaelkoontz/Zotero/storage/58SRZ7LV/Joseph et al. - 2019 - Spatiotemporal prediction of wildfire extremes wit.pdf;/Users/michaelkoontz/Zotero/storage/HCP53C88/384115v3.html}
